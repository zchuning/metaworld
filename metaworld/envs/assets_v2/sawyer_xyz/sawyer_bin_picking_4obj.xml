        <material name="obj_purple" rgba="1 0 1 1" shininess=".2" reflectance="0" specular=".5"/>
    </asset>

    <worldbody>
        <include file="../objects/assets/xyz_base.xml"/>

        <body name="obj" pos="-0.17 0.75 0.04">
            <freejoint/>
            <body childclass="bin_base" name="objA">
                <geom material="obj_green" size="0.02 0.02 0.02" type="box"/>
                <geom class="obj_col" size="0.02 0.02 0.02" type="box" mass=".1"/>
            </body>
        </body>

        <body name="obj2" pos="0.17 0.7 0.04">
            <freejoint/>
            <body childclass="bin_base" name="objB">
                <geom material="obj_teal" size="0.02 0.02 0.02" type="box"/>
                <geom class="obj_col" size="0.02 0.02 0.02" type="box" mass=".1"/>
            </body>
        </body>

        <body name="obj3" pos="-0.07 0.7 0.04">
            <freejoint/>
            <body childclass="bin_base" name="objC">
                <geom material="obj_purple" size="0.02 0.02 0.02" type="box"/>
                <geom class="obj_col" size="0.02 0.02 0.02" type="box" mass=".1"/>
            </body>
        </body>

        <body name="obj4" pos="0.07 0.7 0.04">
            <freejoint/>
            <body childclass="bin_base" name="objD">
                <geom material="obj_yellow" size="0.02 0.02 0.02" type="box"/>
                <geom class="obj_col" size="0.02 0.02 0.02" type="box" mass=".1"/>
            </body>
        </body>


        <body name="bin_start" pos="-0.12 0.7 0">
            <include file="../objects/assets/binA.xml"/>
        </body>

        <body name="bin_goal" pos="0.12 0.7 0">
            <include file="../objects/assets/binB.xml"/>
        </body>


    </worldbody>

    <actuator>
        <position ctrllimited="true" ctrlrange="-1 1" joint="r_close" kp="400"  user="1"/>
        <position ctrllimited="true" ctrlrange="-1 1" joint="l_close" kp="400"  user="1"/>
    </actuator>
    <equality>
        <weld body1="mocap" body2="hand" solref="0.02 1"></weld>
    </equality>
</mujoco>
